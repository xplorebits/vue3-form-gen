{
  "name": "vue3-form-gen",
  "version": "0.0.0",
  "description": "A Vue 3 library for creating, reordering, and exporting dynamic forms with a drag-and-drop interface and JSON schema generation, mirroring popular form-building functionalities.",
  "private": true,
  "type": "module",
  "main": "dist/vue3-form-gen.umd.js",
  "module": "dist/vue3-form-gen.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-form-gen.es.js",
      "require": "./dist/vue3-form-gen.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "vite": "^5.0.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xplorebits/vue3-form-gen.git"
  },
  "keywords": [
    "vue3",
    "forms",
    "survey"
  ],
  "homepage": "https://github.com/xplorebits/vue3-form-gen#readme"
}
